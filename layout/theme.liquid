 {% comment %}
  <!-- snippet reference -->
    <!-- If there's already a meta description like meta name="description", remove it and add this instead -->
       
        {% include 'meta' %}    
  
    <!-- Don't include the head tags, the comments or the dots (...) -->
  {% endcomment %}
  
  {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  <link rel="canonical" href="{{ canonical_url }}">
  
  {% comment %}
  <title>
  {{ page_title }}{% if current_tags %}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
  {% endif %}
  {% endcomment %}

  {% include 'social-meta-tags' %}

  <script>
    document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
  </script>

  {% comment pluginspeed replaced-css DON'T CHANGE %}{{ 'theme.scss.css' | asset_url | stylesheet_tag }}{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'theme.scss.css', tag: 'stylesheet', liquid_tag: true -%}

  <script>
    window.theme = window.theme || {};

    theme.strings = {
      stockAvailable: {{ 'products.product.stock_available' | t: count: '1' | json }},
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      noStockAvailable: {{ 'products.product.stock_unavailable' | t | json }},
      willNotShipUntil: {{ 'products.product.will_not_ship_until' | t: date: '[date]' | json }},
      willBeInStockAfter: {{ 'products.product.will_be_in_stock_after' | t: date: '[date]' | json }},
      totalCartDiscount: {{ 'cart.general.savings_html' | t: price: '[savings]' | json }},
      addressError: {{ 'sections.map.address_error' | t | json }},
      addressNoResults: {{ 'sections.map.address_no_results' | t | json }},
      addressQueryLimit: {{ 'sections.map.address_query_limit_html' | t | json }},
      authError: {{ 'sections.map.auth_error_html' | t | json }}
    };
  </script>

  <!--[if (gt IE 9)|!(IE)]><!-->{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'lazysizes.min.js' | asset_url }}" async="async"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'lazysizes.min.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'lazysizes.min.js' -%}" async="async"></script><!--<![endif]-->
  <!--[if lte IE 9]>{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'lazysizes.min.js' | asset_url }}"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'lazysizes.min.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'lazysizes.min.js' -%}"></script><![endif]-->

  {% if template.directory == 'customers' %}
    <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script><!--<![endif]-->
    <!--[if lte IE 9]><script src="{{ 'shopify_common.js' | shopify_asset_url }}"></script><![endif]-->
  {% endif %}

  <!--[if (gt IE 9)|!(IE)]><!-->{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'vendor.js' | asset_url }}" defer="defer"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'vendor.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'vendor.js' -%}" defer="defer"></script><!--<![endif]-->
  <!--[if lt IE 9]>{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'vendor.js' | asset_url }}"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'vendor.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'vendor.js' -%}"></script><![endif]-->

  {% if template.name == 'product' or template.name == 'cart' %}
    <script>
      window.theme = window.theme || {};
      theme.moneyFormat = {{ shop.money_format | json }};
    </script>
  {% endif %}
  
  <meta name="msvalidate.01" content="7E6D6F1019C7B1A2AC751D5013736E66" />        
    
  <meta name="yandex-verification" content="d8959d713fdd6813" />  

  <meta name="google-site-verification" content="7t8C5Nt8wsFdNdg2d6rXY0pNJKcizo_TcX3FqR2V1Qk" />
    
  <!--[if (gt IE 9)|!(IE)]><!-->{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'theme.js' | asset_url }}" defer="defer"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'theme.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'theme.js' -%}" defer="defer"></script><!--<![endif]-->
  <!--[if lte IE 9]>{% comment pluginspeed replaced-js DON'T CHANGE %}<script src="{{ 'theme.js' | asset_url }}"></script>{% endcomment %}{%- include 'pluginspeed-minify-asset', asset: 'theme.js', tag: 'script' -%} <script src="{%- include 'pluginspeed-asset-url', asset: 'theme.js' -%}"></script><![endif]-->

  <script src="//www.searchanise.com/widgets/shopify/init.js?a=1T3i3B3u3h"></script>

  {% if collection.previous_product %}<link rel="prev" href="{{ collection.previous_product }}">{% endif %}
  {% if collection.next_product %}<link rel="next" href="{{ collection.next_product }}">{% endif %}
   
  {{ content_for_header }}
   {% include 'twitter-card' %}
   {% include 'fb-open-graph' %}
</head>

<body data-template="{{ template }}" class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
{% capture the_snippet_content %}{% include 'socialshopwave-helper' %}{% endcapture %}
            {% unless the_snippet_content contains 'Liquid error' %}
              {{ the_snippet_content }}
            {% endunless %}

  <a class="in-page-link visually-hidden skip-link" href="#MainContent">
    {{ 'general.accessibility.skip_to_content' | t }}
  </a>

  {% section 'header' %}

  <div class="page-container page-element is-moved-by-drawer">
    <main class="main-content" id="MainContent" role="main">
      {{ content_for_layout }}
 {% include 'station-tabs' %}
    </main>
 {% include 'breadcrumbs' %}
    {% section 'footer' %}
  </div>

  <script id="QuantityTemplate" type="text/template">
    {% raw %}
      <div class="js-qty">
        <input type="text" value="{{ value }}" id="{{ key }}" name="{{ name }}" pattern="[0-9]*" data-line="{{ line }}" class="js-qty__input" aria-live="polite">
        <button type="button" class="js-qty__adjust js-qty__adjust--minus" aria-label="{% endraw %}{{ 'cart.label.decrease_quantity' | t }}{% raw %}">
          {% endraw %}{% include 'icon-minus' %}{% raw %}
          <span class="icon__fallback-text">âˆ’</span>
        </button>
        <button type="button" class="js-qty__adjust js-qty__adjust--plus" aria-label="{% endraw %}{{ 'cart.label.increase_quantity' | t }}{% raw %}">
          {% endraw %}{% include 'icon-plus' %}{% raw %}
          <span class="icon__fallback-text">+</span>
        </button>
      </div>
    {% endraw %}
  </script>

  {% if template.name == 'product' %}
    <div id="ProductModal" class="modal">
      <div class="modal__inner">
        <img src="" id="ProductZoomImg" class="product-modal__image">
      </div>
      <button type="button" class="modal__close js-modal-close text-link">
        {% include 'icon-close' %}
        <span class="icon__fallback-text">{{ 'general.accessibility.close_modal' | t | json }}</span>
      </button>
    </div>
  {% endif %}
  
  {% comment %} 
  <script>      
    sswRun(function(){
      setInterval(function() {
        if (ssw.length > 0) {
          ssw('#ssw-addToCartBtn').after($('.ssw-product-btns.ssw-hide'))
          clearInterval();
        }
      }, 300); 
  </script>
  {% endcomment %}
  
    
</body>
</html>
{%- endcapture non_minified_html -%}
{%- assign minifier = 'pluginspeed-minify-html' -%}
{%- capture minifier_output -%}{%- include minifier, minified: non_minified_html -%}{%- endcapture -%}
{%- assign error_message = 'Liquid error: Could not find asset snippets/' | append: minifier -%}
{%- if minifier_output contains error_message -%}
 {{ non_minified_html }}
{%- else -%}
  {{ minifier_output }}
{%- endif -%}
